{
  "Azure": {
    "FunctionsUrl": "https://your-function-app.azurewebsites.net",
    "StorageAccount": "yourstorageaccount",
    "StorageKey": "your-storage-account-key",
    "SqlServer": "your-sql-server.database.windows.net",
    "SqlDatabase": "your-database-name",
    "SqlUser": "your-username",
    "SqlPassword": "your-password",
    "ResourceGroup": "your-resource-group"
  },
  "Authentication": {
    "Google": {
      "ClientId": "your-google-client-id.apps.googleusercontent.com",
      "ClientSecret": "your-google-client-secret",
      "CallbackUrl": "https://your-function-app.azurewebsites.net/auth/google/callback"
    }
  },
  "Storage": {
    "PhotoContainer": "photos",
    "ThumbnailContainer": "thumbnails",
    "MaxFileSize": 10485760,
    "AllowedFormats": ["jpg", "jpeg", "png", "webp"]
  },
  "Database": {
    "ConnectionTimeout": 30,
    "CommandTimeout": 30,
    "EnableLogging": true
  },
  "Application": {
    "Name": "PhotoShare",
    "Version": "1.0.0",
    "Environment": "production"
  }
}
